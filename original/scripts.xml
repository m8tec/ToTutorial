			<wait time="1500"/>
		</step>
		<step name="checkpoint_downhill">
            <waywall x1="2035" y1="2336" z1="0" x2="5114" y2="-235" z2="0" indicatorX="3462" indicatorY="1274" indicatorZ="-600"/>
